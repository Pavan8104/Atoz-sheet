//Queues
//First In First out 
// you can add element fornt and remove from the top 
#include<iostream>
#include<queue>
using namespace std;


int main() {
queue<int> q;
q.push(1);
q.push(2);
q.push(3):
//back elememnt is 3
cout<<"Back ELement : " << q.back() << edl;
//first element is 1
cout<< " Front element " << q.front() << endl;
//Remove the front element 
q.pop() ;
cout<< "New Front element " << q.front() << endl;

return 0 ; 

}




